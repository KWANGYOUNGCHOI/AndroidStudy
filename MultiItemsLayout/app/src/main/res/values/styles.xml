<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiItemsLayout">
        <attr name="mil_title" format="string" />
        <attr name="mil_desc" format="string" />
    </declare-styleable>

    <declare-styleable name="MultiItemsLayout_Layout">
        <attr name="layout_mil_itemPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
        </attr>
    </declare-styleable>
</resources>
